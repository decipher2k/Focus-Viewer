<script type="text/javascript">
  let nlBannerInputActivated = false;

  function showNLBanner() {
    let nlBanner = document.getElementById('nl-banner');
    nlBanner.addEventListener('click', function (event) {
      event.stopPropagation();
    });

    let nlBannerEmailInput = document.getElementById('nl-banner-email-input');
    nlBannerEmailInput.addEventListener('input', function () {
      nlBannerInputActivated = true;
    });

    let nlForm = document.getElementById('nl-banner-form');
    nlForm.addEventListener('submit', function () {
      Golem.createCookie('golem_nlBannerShown', true, 168);
      _gcpx.push(['ev', 'd', 'nlbanner21/submit']);
    });

    let nlBannerClose = document.getElementById('nl-banner-close');
    nlBannerClose.addEventListener('click', function (event) {
      event.preventDefault();
      event.stopPropagation();
      nlBanner.style.display = 'none';
      sessionStorage.setItem('golem_nlBannerShown', 'true');
      Golem.createCookie('golem_nlBannerShown', true, 168);
      _gcpx.push(['ev', 'd', 'nlbanner21/close']);
    });

    if (sessionStorage.golem_nlBannerShown != 'true' && Golem.readCookie('golem_nlBannerShown') != 'true') {
      nlBanner.style.display = '';
      nlBanner.classList.add('animate__bounceInUp');
      window.addEventListener('scroll', hideScrollTrigger);
      hideScrollTrigger(); // instant check
      addTimedAutoHide(nlBanner, 15000);
      _gcpx.push(['ev', 'd', 'nlbanner21/show']);
    } else {
      nlBanner.style.display = 'none';
    }

    window.removeEventListener('scroll', addScrollTrigger);
  }

  function addScrollTrigger() {
    let scrollTop = window.pageYOffset;
    let documentHeight = document.body.scrollHeight;
    let scrolled = (scrollTop / documentHeight) * 100;
    if (scrolled >= 10) {
      showNLBanner();
    }
  }

  function hideScrollTrigger() {
    let nlBanner = document.getElementById('nl-banner'),
      el = document.getElementsByClassName('staticNlForm')[0];
    if (el !== null) {
      let rect = el.getBoundingClientRect();
      if (rect.top <= (window.innerHeight || document.documentElement.clientHeight)) {
        window.removeEventListener('scroll', hideScrollTrigger);
        addTimedAutoHide(nlBanner, 10);
      }
    }
  }

  function addTimedAutoHide(element, time) {
    setTimeout(function () {
      if (!nlBannerInputActivated) {
        if (element.classList.contains('animate__animated')) {
          element.classList.remove('animate__bounceInUp');
          element.classList.add('animate__bounceOutDown');
          setTimeout(function () {
            element.style.display = 'none';
            element.classList.remove('animate__bounceOutDown');
          }, 1000);
        } else {
          element.style.display = 'none';
        }
      }
      sessionStorage.setItem('golem_nlBannerShown', 'true');
    }, time);
  }

  if (false) {
    window.addEventListener('scroll', addScrollTrigger);
  }
</script>
<!-- /nl-banner --></div><!-- #grandwrapper -->
    </div><!-- #golemContentToHide -->
